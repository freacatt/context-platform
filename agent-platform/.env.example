AGENT_PLATFORM_DATABASE_URL=postgresql+psycopg2://user:password@localhost/context_platform

# Qdrant vector store
AGENT_PLATFORM_QDRANT_URL=http://localhost:6333
AGENT_PLATFORM_QDRANT_API_KEY=

# Unified LLM configuration (LangChain)
# Provider can be "openai", "anthropic", "gemini", "grok", "deepseek"
AGENT_PLATFORM_LLM_PROVIDER=anthropic
AGENT_PLATFORM_LLM_MODEL=claude-3-5-sonnet-20241022

# Unified embeddings configuration
# Provider can be "openai", "anthropic", "gemini"
AGENT_PLATFORM_EMBEDDINGS_PROVIDER=anthropic
AGENT_PLATFORM_EMBEDDINGS_MODEL=text-embedding-3-large

# Provider credentials
AGENT_PLATFORM_ANTHROPIC_API_KEY=
AGENT_PLATFORM_OPENAI_API_KEY=
AGENT_PLATFORM_OPENAI_BASE_URL=
AGENT_PLATFORM_GEMINI_API_KEY=
AGENT_PLATFORM_GROK_API_KEY=
AGENT_PLATFORM_GROK_BASE_URL=
AGENT_PLATFORM_DEEPSEEK_API_KEY=
AGENT_PLATFORM_DEEPSEEK_BASE_URL=
